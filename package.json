{
  "name": "opentok-annotation",
  "version": "2.0.59",
  "description": "OpenTok annotation accelerator pack",
  "main": "dist/opentok-annotation.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build:css": "copyfiles css/*.css dist/",
    "build:js": "concat -o dist/opentok-annotation.js src/acc-pack-annotation.js src/annotation-widget.js && uglifyjs dist/opentok-annotation.js -o dist/opentok-annotation.min.js",
    "build:images": "copyfiles images/**/* dist/",
    "build:logversion": "replace '(js-vsol-)(x.y.z)' '$1'$npm_package_version ./dist",
    "build": "mkdir dist && npm run build:js & npm run build:css & npm run build:images && npm run build:logversion",
    "test": "karma start"
  },
  "keywords": [
    "opentok",
    "annotation",
    "annotations",
    "opentok-acc-pack",
    "screen-sharing",
    "audio/video"
  ],
  "author": "adrice727@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/opentok/accelerator-annotation-js"
  },
  "devDependencies": {
    "babel-plugin-espower": "^3.0.1",
    "babelify": "^10.0.0",
    "browserify": "^16.5.2",
    "chai": "^4.2.0",
    "concat": "^3.0.0",
    "copyfiles": "^2.3.0",
    "coverage": "^0.4.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.2",
    "html2js": "^0.2.0",
    "karma": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-mocha": "^2.0.1",
    "karma-requirejs": "^1.1.0",
    "karma-sinon": "^1.0.5",
    "merge-stream": "^2.0.0",
    "mocha": "^8.1.3",
    "replace": "^0.3.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.3",
    "uglify-js": "^3.10.2",
    "watchify": "^3.11.1",
    "yuidocjs": "^0.10.2"
  },
  "dependencies": {
    "jquery": "^3.5.1",
    "opentok-solutions-logging": "^1.1.0",
    "underscore": "^1.11.0"
  }
}